rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow write: if request.auth != null;
    }
    
    match /feed/{docId} {
      allow read: if true;
    }
    
    match /photos/{docId} {
      allow read: if true;
    }
    
    match /quotes/{docId} {
      allow read: if true;
    }
    
    match /songs/{docId} {
      allow read: if true;
    }

    match /profile/{docId} {
      allow read: if true;
      
      match /private/{privId} {
        allow read: if request.auth != null;
      }
    }
  }
}